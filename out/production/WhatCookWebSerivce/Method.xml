<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="entity.Method">
        <table name="method" schema="whatcook" catalog=""/>
        <attributes>
            <id name="id">
                <column name="ID" precision="11"/>
            </id>
            <basic name="step">
                <column name="STEP" nullable="false" precision="11"/>
            </basic>
            <basic name="content">
                <column name="CONTENT" nullable="false" length="1000"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
